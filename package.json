{
  "name": "rent-x",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "bcryptjs": "^3.0.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsyringe": "^4.10.0",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "tsc",
    "dev": "ts-node-dev --inspect --respawn --ignore-watch node_modules --transpile-only src/server.ts",
    "test": "jest",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.spec.ts'",
    "test:unit": "jest --testMatch='**/tests/unit/**/*.spec.ts'",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "migration:run": "migration:run -d ./src/database/index.ts",
    "migration:revert": "migration:revert -d ./src/database/index.ts",
    "migration:create": "migration:create -t"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^6.0.0",
    "csv-parse": "^5.6.0",
    "express": "4.19.2",
    "express-async-errors": "3.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.1.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0"
  }
